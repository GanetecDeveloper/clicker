{
  "name": "clicker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href \"https://GanetecDeveloper.github.io/clicker/\"",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "gendoc": "npx compodoc -p tsconfig.doc.json -s --disablePrivate --disableLifeCycleHooks",
    "deploy": "npm run build:prod && npx angular-cli-ghpages --dir=dist/clicker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "~12.2.0",
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-icon-button": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@material/mwc-top-app-bar-fixed": "^0.20.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
